      <origin xyz="0.0361850676948983 -0.0331675189831137 -1.21207146841943E-08" rpy="0 0 0" />
      <mass value="0.0174460087279995" />
      <inertia
        ixx="1.15871030484505E-06"
        ixy="1.18962145386134E-06"
        ixz="-8.39568250113456E-14"
        iyy="1.41683380026142E-06"
        iyz="6.84207447322716E-14"
        izz="2.39357586071014E-06" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://robotiq_description/meshes/robotiq_2f_140/visual/inner_knuckle.dae" />
      </geometry>
      <material name="">
        <color rgba="0.752941176470588 0.752941176470588 0.752941176470588 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://robotiq_description/meshes/robotiq_2f_140/collision/inner_knuckle.stl" />
      </geometry>
    </collision>
  </link>
</xacro:macro>


